{
  "kind": "collectionType",
  "collectionName": "orders",
  "info": {
    "singularName": "order",
    "pluralName": "orders",
    "displayName": "Order",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "email": {
      "type": "string"
    },
    "companyName": {
      "type": "string"
    },
    "address": {
      "type": "text"
    },
    "city": {
      "type": "string"
    },
    "state": {
      "type": "string"
    },
    "contry": {
      "type": "string"
    },
    "landmark": {
      "type": "string"
    },
    "totalOrderAmount": {
      "type": "decimal"
    },
    "userid": {
      "type": "integer"
    },
    "users_permissions_user": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    },
    "paymentId": {
      "type": "string"
    },
    "OrderItm": {
      "type": "component",
      "repeatable": true,
      "component": "orderitem.order-itm"
    },
    "phone": {
      "type": "string"
    },
    "zip": {
      "type": "string"
    },
    "Status": {
      "type": "enumeration",
      "enum": [
        "Pending",
        "Accepted ",
        "Processing",
        "Dispatched",
        "Delivered",
        "Rejected",
        "Cancelled",
        "Not Paid_COD",
        "Paid"
      ],
      "default": "Pending"
    }
  }
}
